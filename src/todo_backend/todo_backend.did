type Todo = record {
    "id": nat64;
    "text": text;
    "completed": bool;
};

service : {
    "getPaginatedTodos": (nat64, nat64) -> (vec Todo) query;
    "getTodo": (nat64) -> (Todo) query;
    "addTodos": (vec text) -> (nat64);
    "removeTodos": (vec nat64) -> (nat64);
    "toggleTodo": (nat64) -> (bool);
    "updateTodoText": (nat64, text) -> (Todo);
}